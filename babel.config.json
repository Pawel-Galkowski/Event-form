{
  "env": {
    "test": {
      "presets": [["@babel/env", { "targets": { "node": "current" } }]]
    }
  },
  "presets": [
      "@babel/preset-typescript",
      "@babel/preset-react",
    [
      "@babel/preset-env", {
        "targets": {
          "node": "current"
        },
        "modules": "auto"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-react-jsx",
    "@babel/syntax-dynamic-import",
    "@babel/transform-runtime", 
    "css-modules-transform"
  ]
}
